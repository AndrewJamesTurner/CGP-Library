				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library=".\CGP-Library.dll" />
				</Linker>
			</Target>
			<Target title="customFitnessFunction">
				<Option output="gettingStarted" prefix_auto="1" extension_auto="1" />
