				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-DNO_DLL" />
				</Compiler>
			</Target>
			<Target title="createDataSet">
				<Option output="gettingStarted" prefix_auto="1" extension_auto="1" />
