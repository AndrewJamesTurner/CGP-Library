				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add library=".\CGP-Library.dll" />
				</Linker>
			</Target>
			<Target title="manipulatingChromosomes">
				<Option output="gettingStarted" prefix_auto="1" extension_auto="1" />
